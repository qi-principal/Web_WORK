<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.adplatform.module.delivery.mapper.AdDeliveryTaskMapper">


    <update id="updateStatus">
        update ad_delivery_task
        <set>
            status=#{status},
        </set>
        where id=#{taskId}
    </update>
</mapper>